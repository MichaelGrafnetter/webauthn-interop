      <command:example>
        <maml:title>--------------------- EXAMPLE 1 (Entra ID) ---------------------</maml:title>
        <dev:code>PS \&gt; Connect-MgGraph -Scopes 'UserAuthenticationMethod.ReadWrite.All'
PS \&gt; Get-PasskeyRegistrationOptions -UserId 'AdeleV@contoso.com'</dev:code>
        <dev:remarks>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>--------------------- EXAMPLE 2 (Entra ID) ---------------------</maml:title>
